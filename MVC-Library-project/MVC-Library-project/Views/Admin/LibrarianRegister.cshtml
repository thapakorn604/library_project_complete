<html lang="en">

@Html.Partial("_Header")
<body>
    <!-- NAVBAR -->
    @Html.Partial("_Navbar_librarian")
    <!-- END NAV -->
         <!-- BODY -->
        <div class="container">
            <div class="section"></div>
           
                <!--Profile Form-->
                <div class="col s12 m12"><h3>Librarian Register</h3></div>
                <div class="col s12 m12 valign center">
                    <div class="row">
                       <div class="input-field col s12 m6">
                            <i class="material-icons prefix">adb</i>
                            <label name="username">Username</label>
                            <input id="icon_prefix" type="text" class="validate">
                        </div>
                        <div class="col s12 m6">
                            <form action="#">
                                <p class="valign center">
                                <input class="with-gap" name="gender" type="radio" id="male"/>
                                <label for="male">&nbsp;&nbsp;Male&nbsp;&nbsp;</label>
                                <input class="with-gap" name="gender" type="radio" id="female"/>
                                <label for="female">Female</label>
                                </p>
                            </form>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12 m6">
                            <i class="material-icons prefix">lock_open</i>
                            <label name="Password">Password</label>
                            <input id="icon_prefix" type="text" class="validate">
                        </div>
                         <div class="input-field col s12 m6">
                            <i class="material-icons prefix">lock_outlinei</i>
                            <label name="cPassword">Comfirm Password</label>
                            <input id="icon_prefix" type="text" class="validate">
                        </div>
                    </div>

                    <div class="row">
                        <div class="input-field col s12 m6">
                            <i class="material-icons prefix">account_circle</i>
                            <label name="firstName">Firstname</label>
                            <input id="icon_prefix" type="text" class="validate">
                        </div>
                        <div class="input-field col s12 m6">
                            <i class="material-icons prefix">account_circle</i>
                            <label name="lastName">Surname</label>
                            <input id="icon_prefix" type="text" class="validate">
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12 m6">
                            <i class="material-icons prefix">event</i>
                            <label for="bday">Birthday</label>
                            <input type="text" class="datepicker">
                        </div>
                        <div class="input-field col s12 m6">
                            <i class="material-icons prefix">phone</i>
                            <label for="telephone">Telephone</label>
                            <input id="icon_telephone" type="tel" class="validate">
                        </div>
                    </div>
                    <div class="row">
                        
                    </div>
                    <div class="row">
                        <div class="col s12 m12">
                            <button class="btn waves-effect waves-light red" type="submit" name="action">Submit</button>
                            <button class="btn waves-effect waves-light red" type="submit" name="action">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>
        
         <!-- END BODY -->

        <!-- Plus -->
     
    <!-- FOOTER -->
   @Html.Partial("_Footer")
    <!-- END FOOTER -->


</body>
</html>